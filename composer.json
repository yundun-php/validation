{
    "name": "yd/validation",
    "description": "YunDun php validation",
    "homepage": "http://www.yundun.com",
    "keywords": ["valid","php","validate","validation"],
    "authors": [
        {
            "name": "liuguogen",
            "email": "liuguogen@yundun.com",
            "role": "Development"
        }
    ],
    "require": {
        "php": ">=5.6.0"
       
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
       
    },
    "autoload": {
        "psr-4": {
            "Validation\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "config": {
        "process-timeout":      600,
        "preferred-install":    "dist",
        "github-protocols":     ["https"]
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}